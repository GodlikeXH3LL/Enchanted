        end
    end
end)

folder8:AddSwitch("Auto Train All Zones", function(state)
  trainallzone = state
  if trainallzone then
    spawn(function()
        local function MMWT()
            if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(322.912567, 8830.49023, 87.7899475, -0.44533211, -1.57474229e-08, 0.895365477, 5.90717919e-09, 1, 2.05257855e-08, -0.895365477, 1.4429876e-08, -0.44533211)
            end
        end
        local function LP()
            if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-113.318001, 12958.5, 293.145996, 0.778010249, -8.46390602e-09, -0.628251612, -6.35732467e-10, 1, -1.42594345e-08, 0.628251612, 1.14933858e-08, 0.778010249)
            end
        end
        local function T()
            if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(318.627594, 16872.0977, -8.53180027, -0.756375313, 6.09071265e-08, 0.654137909, 9.49819512e-09, 1, -8.21278263e-08, -0.654137909, -5.59063302e-08, -0.756375313)
            end
        end
        local function ET()
            if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(330.810181, 30383.0996, -16.7225971, -0.478063285, -6.45304565e-09, 0.878325403, 4.87447593e-08, 1, 3.38782478e-08, -0.878325403, 5.90097073e-08, -0.478063285)
            end
        end
        while true do
            MMWT()
            wait(3)
            LP()
            wait(3)
            T()
            wait(3)
            ET()
        end
    end)
  end
end)

else
    trainallzone = false
end
end)

local folder9 = MiscV3:AddFolder("Blaze Sell")

folder9:AddSwitch("Auto Blaze Sell", function(Value)
    ABS = Value
    if ABS then
        spawn(function()
            local function BVS()
                if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(85.2215424, 91245.5469, 120.192863, 0.517608643, -3.23137392e-08, -0.855617464, 3.34196812e-08, 1, -1.75492261e-08, 0.855617464, -1.95108321e-08, 0.517608643)
                end
            end
            local function R()
                if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-15.4311571, 63.4752312, 32.3788757, -0.312022954, 4.93972934e-08, -0.950074553, 7.51569189e-08, 1, 2.7310076e-08, 0.950074553, -6.28833021e-08, -0.312022954)
                end
            end
            while true do
                BVS()
                wait(1)
                R()
            end
        end)
    else
        ABS = false
    end
end)
