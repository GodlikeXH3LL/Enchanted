local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/iblameaabis/Ililililililililililililil/refs/heads/main/W%20UI", true))()

local window = library:AddWindow("Enchanted Hub | Ninja", {
    main_color = Color3.fromRGB(1, 2, 3),
    min_size = Vector2.new(655, 650),
    can_resize = false,
})
local Main = window:AddTab("Main")
local folder1 = Main:AddFolder("Time Of Day")

folder1:AddButton("Day Time", function()
    game.Lighting.TimeOfDay = "12:00:00"
end)

folder1:AddButton("Night Time", function()
    game.Lighting.TimeOfDay = "00:00:00"
end)

folder1:AddButton("Dawn", function()
    game.Lighting.TimeOfDay = "09:00:00"
end)

local folder2 = Main:AddFolder("Performance")

folder2:AddButton("Less Lag", function()
    for _, v in pairs(game:GetDescendants()) do
        if v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") then
            v.Enabled = false
        end
    end
    local lighting = game:GetService("Lighting")
    lighting.GlobalShadows = false
    lighting.FogEnd = 9e9
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Parent:FindFirstChild("Humanoid") then
            v.Material = Enum.Material.SmoothPlastic
            if v:IsA("Texture") then
                v:Destroy()
            end
        end
    end
    settings().Rendering.QualityLevel = 1
end)

folder2:AddButton("FPS", function()
    RemoveLag()
    if cleanupLoop then
        task.cancel(cleanupLoop)
        cleanupLoop = nil
    end
    cleanupLoop = task.spawn(function()
        while true do
            wait(50)
            RemoveLag()
        end
    end)
end)

folder2:AddSwitch("Hide Frames", function(Value)
    hideframes = Value
    if hideframes then
        local rSto = game:GetService("ReplicatedStorage")
        for _, obj in pairs(rSto:GetChildren()) do
            if obj.Name:match("Frame$") then
                obj.Visible = not Value
            end
        end
    end
    if hideallframes then
        task.cancel(hideallframes)
    end
end)

local AutoFarm = window:AddTab("Auto Farm")
local folder3 = AutoFarm:AddFolder("Auto Farm")

folder3:AddSwitch("Auto Swing", function(state)
    _G.swing = state
    if _G.swing then
        spawn(function()
            while _G.swing do
                wait()
                local args = { [1] = "swingKatana" }
                game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(unpack(args))
            end
        end)
    end
end)

folder3:AddSwitch("Auto Farm Chi", function(state)  
    _G.c = state
    if _G.c then
        spawn(function()
            while _G.c do
                wait()
                for _, v in pairs(game.Workspace.spawnedCoins.Valley:GetChildren()) do
                    if v.Name == "Blue Chi Crate" then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Position)
                        wait(0.3)
                    end
                end
            end
        end)
    end
end)

folder3:AddSwitch("Auto Farm Coins", function(state)  
    _G.co = state
    if _G.co then
        spawn(function()
            while _G.co do
                wait()
                for _, v in pairs(game.Workspace.spawnedCoins.Valley:GetChildren()) do
                    if v.Name == "Purple Coin Crate" then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Position)
                        wait(0.3)
                    end
                end
            end
        end)
    end
end)    

folder3:AddSwitch("Auto Buy Hoops", function(state)
    _G.hoops = state
    if _G.hoops then
        spawn(function()
            while _G.hoops do
                wait()
                local plr = game.Players.LocalPlayer
                for _, v in pairs(workspace.Hoops:GetDescendants()) do
                    if v.ClassName == "MeshPart" and v:FindFirstChild("touchPart") then
                        v.touchPart.CFrame = plr.Character.HumanoidRootPart.CFrame
                    end
                end
            end
        end)
    end
end)    

folder3:AddSwitch("Auto Sell", function(state)
    _G.sell = state
    if _G.sell then
        spawn(function()
            while _G.sell do
                wait()
                local player = game.Players.LocalPlayer
                local sellCircle = game.workspace.sellAreaCircles["sellAreaCircle15"]
                if sellCircle and sellCircle:FindFirstChild("circleInner") then
                    sellCircle.circleInner.CFrame = player.Character.HumanoidRootPart.CFrame
                end
                wait()
                if sellCircle and sellCircle:FindFirstChild("circleInner") then
                    sellCircle.circleInner.CFrame = game.Workspace.Part.CFrame
                end
            end
        end)
    end
end)

folder3:AddSwitch("Auto Sell When Full", function(state)
    _G.sell = state
    if _G.sell then
        spawn(function()
            while _G.sell do
                wait()
                if game.Players.LocalPlayer.PlayerGui.gameGui.maxNinjitsuMenu.Visible then
                    local sellCircle = game.workspace.sellAreaCircles["sellAreaCircle15"]
                    if sellCircle and sellCircle:FindFirstChild("circleInner") then
                        sellCircle.circleInner.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                    end
                    wait()
                    if sellCircle and sellCircle:FindFirstChild("circleInner") then
                        sellCircle.circleInner.CFrame = game.Workspace.Part.CFrame
                    end
                end
            end
        end)
    end
end)

